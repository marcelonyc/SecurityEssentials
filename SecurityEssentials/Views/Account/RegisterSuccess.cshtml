@{
    ViewBag.Title = "Register Success";
    ViewBag.Active = "Home";
}

<div class="container">
    <div class="row">
        <div class="col-md-12"><h1>Registration Pending</h1></div>
    </div>
    <div class="row">
        <div class="col-md-12">Your account registration request has been submitted for processing. An email has been sent to your registered email address to complete the next steps</div>
    </div>        
    <div class="row">
        <div class="col-md-12">Click @Html.ActionLink("Here to Login", "LogOn", "Account")</div>
    </div>        
</div>

