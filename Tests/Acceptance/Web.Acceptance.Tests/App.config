    <add name="DefaultConnection" connectionString="Server=.;Database=SecurityEssentials;Integrated Security=SSPI;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="RestoreDatabaseAfterTests" value="true" />
    <add key="TimeoutMinutes" value="1" />
    <add key="WaitIntervalSeconds" value="30" />
    <add key="WebServerUrl" value="http://localhost:4845/" />
    <add key="TakeScreenShotOnFailure" value="true" />
    <add key="TestScreenCaptureStorage" value="C:/Temp/" />
    <!-- Application settings -->
    <add key="DefaultHashStrategy" value="0" />
    <add key="EncryptionPassword" value="CHANGE ME TO SOMETHING CRYPTOGRAPHICALLY SECURE" />
    <add key="EncryptionIterationCount" value="103" />
    <!-- Used to proxy tests through OWASP ZAP -->
    <add key="WebBrowserProxy" value="" />
    <!-- Acceptable values are Chrome, Headless Chrome, FireFox, IE -->
    <add key="WebBrowserType" value="Headless Chrome" />
  </appSettings>  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <entityFramework>
